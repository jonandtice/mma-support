{
  "name": "mma-support",
  "displayName": "MMA Support",
  "description": "Syntax highlighting for Musical MIDI Accompaniment files.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "mma",
      "aliases": ["MMA", "mma"],
      "extensions": [".mma"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "mma",
      "scopeName": "source.mma",
      "path": "./syntaxes/mma.tmLanguage.json"
    }]
  }
}
